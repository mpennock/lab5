<%- include partials/header.ejs %>
<h1>Users</h1>
    	<table class="table table-striped table-hover">
    		<thead>
    			<tr>
    				<th>ID</th>
    				<th>Username</th>
    			</tr>
    		</thead>
    		<tbody>    
    		<% for (var i=0; i < users.length; i++) { %>
    			<tr>
    				<td><%= users[i]._id %></td>
    				<td><%= users[i].username %></td>
    			</tr>
    		<% } %>
    		</tbody>
    	</table>
<%- include partials/footer.ejs %>
</body>
</html>